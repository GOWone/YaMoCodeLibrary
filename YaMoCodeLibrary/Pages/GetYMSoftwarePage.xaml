<Page
    x:Class="YaMoCodeLibrary.Pages.GetYMSoftwarePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:YaMoCodeLibrary.Pages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="GetYMSoftwarePage"
    d:DesignHeight="450"
    d:DesignWidth="800"
    Background="{x:Null}"
    mc:Ignorable="d">
    <Border Background="White" CornerRadius="0 0 10 0">
        <Grid>
            <StackPanel
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Orientation="Vertical">
                <Image
                    Width="270"
                    Height="270"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Source="/ymsoft.png" />
                <Label
                    HorizontalAlignment="Center"
                    Content="正在开发中..."
                    FontFamily="/Assets/font/#Alimama DaoLiTi"
                    FontSize="25" />
            </StackPanel>

        </Grid>
    </Border>
</Page>
